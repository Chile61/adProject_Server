<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.common.persistence.dao.mapping.OrderMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.common.persistence.model.Order">
		<id column="id" property="id" />
		<result column="sn" property="sn" />
		<result column="userId" property="userId" />
		<result column="shopId" property="shopId" />
		<result column="money" property="money" />
		<result column="points" property="points" />
		<result column="createTime" property="createTime" />
		<result column="shipAddress" property="shipAddress" />
		<result column="shipName" property="shipName" />
		<result column="shipMobile" property="shipMobile" />
		<result column="remark" property="remark" />
		<result column="state" property="state" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, sn, userId, shopId, money, points, createTime, shipAddress, shipName, shipMobile, remark, state
    </sql>

</mapper>
