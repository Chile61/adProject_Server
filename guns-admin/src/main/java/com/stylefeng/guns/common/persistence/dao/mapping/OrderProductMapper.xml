<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.common.persistence.dao.OrderProductMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.common.persistence.model.OrderProduct">
		<id column="id" property="id" />
		<result column="orderId" property="orderId" />
		<result column="productId" property="productId" />
		<result column="num" property="num" />
		<result column="price" property="price" />
		<result column="point" property="point" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, orderId, productId, num, price, point
    </sql>

</mapper>
