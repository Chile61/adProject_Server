<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.rest.common.persistence.dao.ConversionSumLogMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.rest.common.persistence.model.ConversionSumLog">
		<id column="id" property="id" />
		<result column="points" property="points" />
		<result column="cloudPoints" property="cloudPoints" />
		<result column="onlyPayPoints" property="onlyPayPoints" />
		<result column="dailyCloudConversionRate" property="dailyCloudConversionRate" />
		<result column="dailyCloudConverPoints" property="dailyCloudConverPoints" />
		<result column="dailyConsumptionConversionRate" property="dailyConsumptionConversionRate" />
		<result column="dailyConsumptionConverPoints" property="dailyConsumptionConverPoints" />
		<result column="newPoints" property="newPoints" />
		<result column="newCloudPoints" property="newCloudPoints" />
		<result column="newOnlyPayPoints" property="newOnlyPayPoints" />
		<result column="createTime" property="createTime" />
		<result column="succeed" property="succeed" />
		<result column="message" property="message" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, points, cloudPoints, onlyPayPoints, dailyCloudConversionRate, dailyCloudConverPoints, dailyConsumptionConversionRate, dailyConsumptionConverPoints, newPoints, newCloudPoints, newOnlyPayPoints, createTime, succeed, message
    </sql>

	<select id="getLast" resultType="map">
		select
		<include refid="Base_Column_List" />
		from t_conversion_sum_log where id in (select max(id) from t_conversion_sum_log)
	</select>

</mapper>
